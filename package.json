{
  "name": "styleguide",
  "version": "0.2.0",
  "description": "sample project that may serve as module library and styleguide",
  "license": "MIT",
  "author": "Christopher Voigt <chlorophyllkid@gmail.com>",
  "contributors": [
    "Anke HÃ¤slich <a.haeslich@queo-group.com>",
    "Hannes Liehr <h.liehr@queo-group.com>"
  ],
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "prestart": "rimraf app/",
    "start": "node tasks/watch.js",
    "prebuild": "rimraf app/",
    "build": "npm run build:icon && npm run build:css && npm run build:html && npm run build:img && npm run build:js && npm run build:static",
    "build:color": "node tasks/build/color.js",
    "build:icon": "node tasks/build/icon.js",
    "build:css": "node tasks/build/css.js",
    "build:html": "node tasks/build/html.js",
    "build:img": "node tasks/build/image.js",
    "build:js": "node tasks/build/javascript.js",
    "build:static": "node tasks/build/static.js",
    "test": "npm run lint",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:markdown",
    "lint:css": "stylelint -q ./src/**/*.scss",
    "lint:js": "eslint --quiet ./src/**/*.js ./tasks/**/*.js",
    "lint:markdown": "remark -q . ./src/**/*.md",
    "preversion": "npm test && rimraf dist/",
    "version": "cross-env NODE_ENV=production npm run build && git add -A dist",
    "version:patch": "node tasks/version.js patch && git add -u",
    "version:minor": "node tasks/version.js minor && git add -u",
    "version:major": "node tasks/version.js major && git add -u",
    "dist-patch": "npm run version:patch | npm version patch -m \"upgrade assets to %s\"",
    "dist-minor": "npm run version:minor | npm version minor -m \"upgrade assets to %s\"",
    "dist-major": "npm run version:major | npm version major -m \"upgrade assets to %s\""
  },
  "devDependencies": {
    "app-root-path": "2.1.0",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "browser-sync": "2.26.3",
    "chalk": "2.4.2",
    "cross-env": "5.2.0",
    "eslint": "5.15.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "glob": "7.1.3",
    "imagemin": "6.1.0",
    "imagemin-svgo": "7.0.0",
    "node-notifier": "5.4.0",
    "node-sass": "4.11.0",
    "node-sass-tilde-importer": "1.0.2",
    "node-watch": "0.6.0",
    "pug": "2.0.3",
    "pug-dependency": "1.0.1",
    "remark-cli": "6.0.1",
    "remark-preset-lint-recommended": "3.0.2",
    "rimraf": "2.6.3",
    "rollup": "1.6.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-uglify": "6.0.2",
    "semver": "5.6.0",
    "shelljs": "0.8.3",
    "stylelint": "9.10.1",
    "stylelint-order": "2.1.0",
    "stylelint-scss": "3.5.4",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "cookieconsent": "3.1.0",
    "github-markdown-css": "3.0.1",
    "highlight.js": "9.15.6",
    "marked": "0.6.1",
    "normalize.css": "8.0.1"
  }
}